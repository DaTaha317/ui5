<mvc:View
    controllerName="ui5.controller.ValueHelper"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:custom="ui5.controls"
>
    <VBox>
        <Label
            text="Suppliers"
            labelFor="valueHelperSupplier"
        />
        <custom:ValueHelperWrapper
            id="valueHelperSupplier"
            config="{viewModel>/SupplierConfig}"
        />

        <Label
            text="Emplopyee"
            labelFor="valueHelperEmplopyee"
        />
        <custom:ValueHelperWrapper
            id="valueHelperEmplopyee"
            config="{viewModel>/EmployeeConfig}"
        />
    </VBox>
</mvc:View>
